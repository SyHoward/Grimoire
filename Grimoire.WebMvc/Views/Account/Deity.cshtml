@model List<Grimoire.Models.Deity.DeityRead>

@{
    ViewData["Title"] = "User Deities";
}

<div class="d-flex justify-content-between align-items-center mb-2">
    <h1 class="display-4">Add Deities for @User.Identity?.Name</h1>
</div>
<form>
<table class="table table-success table-striped border">
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var deity in Model)
        {
            <tr>
                <td>
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                        <label class="form-check-label" for="flexCheckDefault">
                            @deity.Name
                        </label>
                    </div>
                </td>
                <td class="text-end">
                </td>
            </tr>
        }
    </tbody>
</table>



</form>
